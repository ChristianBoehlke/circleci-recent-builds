{
  "name": "circleci-recent-builds",
  "version": "0.0.10",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ChristianBoehlke/circleci-recent-builds"
  },
  "bin": {
    "cci-watch": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build ./src/index.tsx -w -o dist/",
    "prerelease": "rm -rf ./dist/",
    "release": "ncc build ./src/index.tsx -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "yarn release"
  },
  "devDependencies": {
    "@types/boxen": "3.0.1",
    "@types/chalk": "2.2.0",
    "@types/cli-spinners": "1.3.0",
    "@types/conf": "3.0.0",
    "@types/ink": "2.0.3",
    "@types/node": "12.12.14",
    "@types/react": "^16.9.13",
    "@types/update-notifier": "2.5.0",
    "@types/window-size": "0.2.4",
    "@zeit/ncc": "0.20.5",
    "axios": "0.19.0",
    "boxen": "4.1.0",
    "conf": "6.2.0",
    "ink": "2.6.0",
    "ink-spinner": "3.0.1",
    "moment": "2.24.0",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "typescript": "3.7.2",
    "window-size": "1.1.1"
  },
  "dependencies": {
    "update-notifier": "3.0.1"
  }
}
